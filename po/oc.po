#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Sintaxi : %s <rep>\n"

#: ../backends/gconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Utilizacion : %s <rep>\n"
"  Fusiona una ierarquia de fichièrs del tipe :\n"
"    rep/%%gconf.xml\n"
"        sosrep1/%%gconf.xml\n"
"        sosrep2/%%gconf.xml\n"
"  en :\n"
"    rep/%%gconf-tree.xml\n"

